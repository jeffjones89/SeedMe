<%= form_for [@user, @company] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :industry %>
  <%= f.text_field :industry %>

  <%= f.label :pitch_url, "Video Link"%>
  <%= f.text_field :pitch_url %>

  <%= f.label :pitch_text %>
  <%= f.text_area :pitch_text %>

  <%= f.submit %>
<% end %>
